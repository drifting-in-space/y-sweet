{
    "name": "@y-sweet/react",
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
        "dev": "npm run build -- --watch",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx}\""
    },
    "version": "0.0.2",
    "description": "React library for building collaboration features with y-sweet.",
    "main": "./dist/main.js",
    "types": "./dist/main.d.ts",
    "exports": {
        ".": {
            "import": "./dist/main.js",
            "module": "./dist/main.js",
            "types": "./dist/main.d.ts"
        }
    },
    "files": [
        "dist/**/*"
    ],
    "homepage": "https://github.com/drifting-in-space/y-sweet",
    "repository": {
        "type": "git",
        "url": "https://github.com/drifting-in-space/y-sweet.git"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/react": "^18.2.14",
        "prettier": "^3.0.0",
        "typescript": "^5.1.6"
    },
    "bugs": {
        "url": "https://github.com/drifting-in-space/y-sweet/issues"
    },
    "peerDependencies": {
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
        "yjs": "^13.0.0"
    },
    "dependencies": {
        "@y-sweet/sdk": "0.0.2",
        "y-protocols": "^1.0.5",
        "y-websocket": "^1.5.0"
    }
}
